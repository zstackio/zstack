                                    <targetPath>WEB-INF/</targetPath>
                                    <includes>
                                        <include>urlrewrite.xml</include>
                                    </includes>
                                </resource>
                                <resource>
                                    <directory>../premium/conf</directory>
                                    <targetPath>WEB-INF/classes</targetPath>
                                    <includes>
                                        <include>**/*</include>
                                    </includes>
                                </resource>
                                <resource>
